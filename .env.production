# 本番ビルド用最小環境変数
# これらの値はダミーですが、ビルドを通すために必要です

# NextAuth設定
NEXTAUTH_SECRET=production-dummy-secret-32-characters-minimum
NEXTAUTH_URL=https://hexamind.ai

# Supabase設定（ダミー）
NEXT_PUBLIC_SUPABASE_URL=https://dummy.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=dummy-anon-key
SUPABASE_SERVICE_ROLE_KEY=dummy-service-key

# Stripe設定（ダミー）
STRIPE_SECRET_KEY=sk_test_dummy
STRIPE_PUBLISHABLE_KEY=pk_test_dummy
STRIPE_WEBHOOK_SECRET=whsec_dummy

# AI API設定（ダミー）
OPENAI_API_KEY=sk-dummy-openai-key
NEXT_PUBLIC_GEMINI_API_KEY=dummy-gemini-key
GOOGLE_AI_API_KEY=dummy-google-ai-key

# Google OAuth設定（ダミー）
GOOGLE_CLIENT_ID=dummy-google-client-id
GOOGLE_CLIENT_SECRET=dummy-google-client-secret

# Sentry設定（オプション）
SENTRY_DSN=

# 機能フラグ
NODE_ENV=production
NEXT_PUBLIC_ENABLE_PREMIUM_MODE=true
NEXT_PUBLIC_ENABLE_USAGE_TRACKING=true